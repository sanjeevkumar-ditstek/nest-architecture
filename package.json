{
  "name": "nest-js-sample",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "husky install && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:up": "ts-node -O '{\"module\":\"commonjs\"}' ./node_modules/migrate-mongo/bin/migrate-mongo up",
    "migrate:down": "ts-node -O '{\"module\":\"commonjs\"}' ./node_modules/migrate-mongo/bin/migrate-mongo down",
    "seed": "ts-node src/db/seeder/seeder.ts",
    "prepare": "husky && husky install"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.5",
    "@nestjs/platform-socket.io": "^10.4.15",
    "@nestjs/sequelize": "^10.0.1",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.15",
    "@sendgrid/mail": "^8.1.3",
    "aws-sdk": "^2.1691.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "continuation-local-storage": "^3.2.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "firebase-admin": "^12.7.0",
    "google-auth-library": "^9.14.2",
    "joi": "^17.13.3",
    "migrate": "^2.1.0",
    "migrate-mongo": "^11.0.0",
    "mjml": "^4.15.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mongo-seeding": "^4.0.1",
    "mongoose": "^8.7.2",
    "mongoose-seed": "^0.6.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.11.5",
    "nest-winston": "^1.9.7",
    "nodemailer": "^6.9.15",
    "parse": "^3.5.1",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "socket.io-client": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.3.3",
    "verify-apple-id-token": "^3.1.2",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/mongodb": "^4.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/passport-apple": "^2.0.3",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.0.0",
    "sequelize-cli": "^6.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
